<?xml version="1.0" encoding="UTF-8"?>

<Configuration status="warn">
    <Appenders>
        <Console name="Console" target="SYSTEM_OUT">
            <PatternLayout pattern="%d %p [%c] - %m%n" />
        </Console>
    </Appenders>
    <Loggers>
        <Root level="INFO">
            <AppenderRef ref="Console" />
        </Root>
        <Root level="ERROR">
            <AppenderRef ref="Console" />
        </Root>
        <!--<Root level="WARN">-->
            <!--<AppenderRef ref="Console" />-->
        <!--</Root>-->
        <!--<Root level="DEBUG">-->
            <!--<AppenderRef ref="Console" />-->
        <!--</Root>-->
    </Loggers>
</Configuration>

